<%%= twitter_bootstrap_form_for(@<%= singular_table_name %>) do |f| %>
	<%= f.inputs do %>
  	<%- attributes.each do |attribute| -%>
    	<%%= f.<%= attribute.reference? ? :association : :input %> :<%= attribute.name %> %>
  	<%- end -%>
  <% end %>
	<%= f.actions do %>
		<%= f.submit "Submit" %>
	<% end %>
<%% end %>