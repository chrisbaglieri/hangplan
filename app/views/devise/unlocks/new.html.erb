<h1>Resend unlock instructions</h1>
<%= twitter_bootstrap_form_for(resource, :as => resource_name, :url => unlock_path(resource_name), :html => { :method => :post }) do |f| %>
	<%= f.inputs do %>
		<%= f.email_field :email, :required => true, :placeholder => 'me@example.com' %>
	<% end %>
	<%= f.actions do %>
  	<%= f.submit "Resend unlock instructions" %>
	<% end %>
<% end %>
<%= render :partial => "devise/shared/links" %>